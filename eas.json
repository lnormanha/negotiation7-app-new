{
  "cli": {
    "version": ">= 4.1.2"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "emulator": {
      "android": {
        "buildType": "apk"
      }
    },
    "release": {
      "android": {
        "gradleCommand": ":app:assembleRelease"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./google-play-negotiation7-api-key.json",
        "track": "internal"
      }
    }
  }
}
